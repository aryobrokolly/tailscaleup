
config tailscale 'settings'
	option enabled '1'
	option port '41641'
	option config_path '/etc/tailscale'
	option fw_mode 'nftables'
	option log_stdout '1'
	option log_stderr '1'
	option acceptRoutes '1'
	option acceptDNS '1'
	option advertiseExitNode '0'
	list access 'tsfwlan'
	list access 'tsfwwan'
	list access 'lanfwts'
	option advertiseRoutes '192.168.100.0/24'

